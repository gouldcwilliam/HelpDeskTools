    </Setting>
    <Setting Name="_EmailFooter" Type="System.String" Scope="User">
      <Value Profile="(Default)">&lt;/table&gt;&lt;/body&gt;&lt;/html&gt;</Value>
    </Setting>
    <Setting Name="_To" Type="System.String" Scope="User">
      <Value Profile="(Default)">chad.gould@wwwinc.com</Value>
    </Setting>
    <Setting Name="_Subject" Type="System.String" Scope="User">
      <Value Profile="(Default)">File Copier Results</Value>
    </Setting>
  </Settings>
